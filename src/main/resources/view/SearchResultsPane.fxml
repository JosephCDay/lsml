<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.String?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Region?>

<fx:root type="BorderPane" onKeyReleased="#keyRelease" xmlns="http://javafx.com/javafx/8.0.65" xmlns:fx="http://javafx.com/fxml/1"
  fx:controller="org.lisoft.lsml.view_fx.SearchResultsPane">
  <top>
    <HBox BorderPane.alignment="CENTER">
      <styleClass>
        <String fx:value="material"/>
        <String fx:value="header"/>
      </styleClass>
      <children>
        <Label styleClass="h1" text="Search Results"/>
        <Region HBox.hgrow="ALWAYS"/>
        <Button mnemonicParsing="false" onAction="#closeWindow" styleClass="accent-button" text="Close"/>
      </children>
    </HBox>
  </top>
  <center>
    <TableView fx:id="results" BorderPane.alignment="CENTER">
      <columns>
      </columns>
    </TableView>
  </center>
</fx:root>
