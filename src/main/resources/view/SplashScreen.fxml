<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.VBox?>


<VBox stylesheets="@BaseStyle.css" xmlns="http://javafx.com/javafx/8.0.60" xmlns:fx="http://javafx.com/fxml/1"
      fx:controller="org.lisoft.lsml.view_fx.controllers.SplashScreenController">
    <ImageView pickOnBounds="true" preserveRatio="true">
        <Image url="@../splash.png"/>
    </ImageView>
    <VBox styleClass="default-padding-left">
        <Label fx:id="progressText" text="Label"/>
        <Label fx:id="progressSubText" text="Label"/>
    </VBox>
</VBox>
